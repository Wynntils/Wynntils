{
  "schemaVersion": 1,
  "authors": [
    "Team Wynntils"
  ],
  "breaks": {
    "exordium": "*"
  },
  "conflicts": {
    "avomod": "*",
    "chatpatches": "*"
  },
  "contact": {
    "homepage": "https://wynntils.com",
    "issues": "https://github.com/Wynntils/Wynntils/issues",
    "sources": "https://github.com/Wynntils/Wynntils"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.crowdin": "https://crowdin.com/project/wynntils-mod",
        "modmenu.discord": "https://discord.gg/SZuNem8",
        "modmenu.github_releases": "https://github.com/Wynntils/Wynntils/releases",
        "modmenu.modrinth": "https://modrinth.com/mod/wynntils",
        "modmenu.patreon": "https://patreon.com/Wynntils"
      },
      "update_checker": true
    }
  },
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${minecraft_version}"
  },
  "description": "The best Wynncraft mod you'll probably find!",
  "entrypoints": {
    "client": [
      "com.wynntils.fabric.WynntilsModFabric"
    ],
    "modmenu": [
      "com.wynntils.fabric.config.ModMenuIntegration"
    ]
  },
  "environment": "*",
  "icon": "assets/wynntils/logo.png",
  "id": "${mod_id}",
  "license": "LGPL-3.0-only",
  "mixins": [
    "wynntils.mixins.json",
    "wynntils.mixins.fabric.json"
  ],
  "name": "Wynntils",
  "suggests": {
    "flamingo": "*"
  },
  "version": "${mod_version}"
}
